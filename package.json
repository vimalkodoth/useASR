{
  "name": "react-asr",
  "version": "0.0.4",
  "description": "ReactJS hook to detect human voice, record and retrieve in wav format with native browser APIs",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "prepare": "npm run build",
    "prepublishOnly": "npm run prettier && npm run lint"
  },
  "files": [
    "dist",
    "index.js",
    "index.d.ts",
    "README.md"
  ],
  "keywords": [
    "react",
    "typescript",
    "prompt ui",
    "prompt engineering",
    "chatgpt"
  ],
  "author": "Vimal Madhavan",
  "license": "MIT",
  "dependencies": {
    "audiobuffer-to-wav": "^1.0.0"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/audiobuffer-to-wav": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "babel": "^6.23.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.8",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "@types/react": "^18.2.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vimalkodoth/useASR.git"
  }
}
